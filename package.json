{
  "name": "greenerator",
  "version": "0.1.2",
  "description": "Call of Cthulhu character generator",
  "scripts": {
    "start": "node index.js",
    "postinstall": "./node_modules/.bin/bower install"
  },
  "dependencies": {
    "express": "4.9.2",
    "bower": "1.3.11"
  },
  "engines": {
    "node": "0.10.32"
  },
  "browser": {
    "jquery": "./build/javascripts/jquery.js",
    "react": "./build/javascripts/react-with-addons.js",
    "underscore": "./build/javascripts/underscore.js",
    "backbone": "./build/javascripts/backbone.js",
    "react-backbone": "./build/javascripts/react-backbone.js",
    "react-mixin-manager": "./build/javascripts/react-mixin-manager.js",
    "backbone-computed-fields": "./build/javascripts/backbone.computedfields.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "react": "React",
    "underscore": "_",
    "backbone": "Backbone",
    "backbone-computed-fields": "ComputedFields"
  },
  "dependencies": {
    "express": "4.9.2",
    "serve-favicon": "2.1.4",
    "morgan": "*",
    "cookie-parser": "*",
    "body-parser": "*",
    "jade": "*",
    "browserify": "5.11.0",
    "browserify-shim": "3.7.0",
    "coffee-script": "1.8.0",
    "coffeeify": "0.7.0",
    "connect": "~2.14.3",
    "gulp": "3.8.7",
    "gulp-changed": "~0.3.0",
    "gulp-ruby-sass": "0.7.1",
    "gulp-imagemin": "~0.1.5",
    "gulp-livereload": "~1.2.0",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp-util": "*",
    "gulp-autoprefixer": "*",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1",
    "bower": "1.3.11",
    "main-bower-files": "*"
  }
}
